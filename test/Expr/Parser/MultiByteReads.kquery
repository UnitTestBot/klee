# RUN: %kleaver -print-ast -pc-multibyte-reads=true %s > %t.log
# RUN: grep -q "(ReadLSB w32 4 0)" %t.log
# RUN: grep -q "(ReadMSB w32 2 1)" %t.log

0 : (array (w64 8) (makeSymbolic arr1 1))
1 : (array (w64 8) (makeSymbolic arr2 1))

(query [(Not (Slt 100
                  (Concat w32 (Read w8 7 0)
                              (Concat w24 (Read w8 6 0)
                                          (Concat w16 (Read w8 5 0) (Read w8 4 0))))))]
       false)


(query [(Not (Slt 100
                  (Concat w32 (Read w8 2 1)
                              (Concat w24 (Read w8 3 1)
                                          (Concat w16 (Read w8 4 1) (Read w8 5 1))))))]
       false)

